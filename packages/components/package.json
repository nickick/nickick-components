  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@nickick/typescript-config": "*",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "latest"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "clsx": "^2.1.1",
    "motion": "^11.16.4",
    "tailwind-merge": "^2.6.0"
  }
